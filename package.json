{
  "name": "tiktok-chat-reader",
  "version": "2.0.0",
  "description": "TikTok LIVE Chat Reader with Clean Architecture",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "ts-node -r tsconfig-paths/register src/main.ts",
    "dev:watch": "nodemon --exec ts-node -r tsconfig-paths/register src/main.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "build:exe": "bash build-exe-pkg-ts.sh"
  },
  "dependencies": {
    "@types/ws": "^8.18.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "socket.io": "^4.4.1",
    "tiktok-live-connector": "^2.1.1-beta1",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
