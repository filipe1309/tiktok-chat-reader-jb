# Environment Configuration
# Copy this file to .env and modify as needed

# Server port (default: 8081)
PORT=8081

# TikTok Session ID (optional - for authenticated requests)
# SESSIONID=your_session_id_here

# Rate limiting (default: disabled)
ENABLE_RATE_LIMIT=false
MAX_CONNECTIONS=10
MAX_REQUESTS_PER_MINUTE=5

# TikFinity WebSocket Fallback (used when tiktok-live-connector hits rate limits)
# TIKFINITY_WS_ENDPOINT=wss://tikfinity.zerody.one/tiktok/dapi

# Environment
NODE_ENV=development
